<div class="row">
  <h1>Admin dashboard</h1>
<p class="center">
  <% if admin_signed_in? %>
    <%= link_to 'Sidekiq Dashboard', sidekiq_web_path %> <br>
    <%= link_to 'Log out', destroy_admin_session_path, method: :delete, class: "btn btn-danger btn-lg" %>
  <% else %>
    <%= link_to 'Log in', new_admin_session_path, class: "btn btn-success btn-lg" %>
  <% end %>
</p>
</div><!-- /row -->
